package intwquestions;

class ParentClass {

	public void audioSystem() {
		System.out.println("parent system");
	}
}

class ChildClass {

	public void audioSystem() {
		System.out.println("child system");
	}
}

class MainClass	extends ChildClass{
	public static void main(String[] args)	{
		ChildClass childClass = new ChildClass();
		childClass.audioSystem();
	}
}
