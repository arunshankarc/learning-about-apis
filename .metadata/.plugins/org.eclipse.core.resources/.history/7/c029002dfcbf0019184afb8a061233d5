package intwquestions;

import java.util.ArrayList;

public class WrappingandUnwrapping {

	public static void main(String[] args) {
		
		// Wrapping 
		float fl = 18.6f; 
		Float fl1 = new Float(fl);
		// autoboxing is automatic wrapping by compiler or help from compiler
		Float fl2 = fl;
		System.out.println(fl1);
		System.out.println(fl2);
		
		// unboxing conversion from wrapper classes to primitive data types
		Character ch = 'a';
		char c = ch;
		ArrayList<Character> aList = new ArrayList<>();
		aList.add(c)
		System.out.println(c);
	}

}
